(defun initialize ()
	(setf albert (make-person 'albert 'union '(money1))))

(defun make-person (name location money)
	(lambda (message)
	      (cond ((equal message 'name) name)
		        ((equal message 'location) location)
                        ((equal message 'money) money))))

(defun send (object message args)
	(funcall (ask object message) args))
